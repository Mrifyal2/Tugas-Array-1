#include <iostream>
using namespace std;
int main() {
  int nilai [5];
  int i;
  int hasil=0;
  for (i=0; i<5; i++) 
  {
    cout << "Input nilai ke-" << i << " : ";
    cin >> nilai[i];
  }
  for (i=0; i<5; ++i)
  {
    hasil+=nilai[i];
    cout << "Hasil ke-" << i << " : " << hasil << endl;
  }
  cout << endl << "Jumlah seluruh : " <<hasil << endl;
  return 0;
}
